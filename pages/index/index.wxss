/**index.wxss**/
.section{
  width: 100%
}
.searchBar{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100rpx;
  background: #fff;
}
.searchBar .searchBar-box{
  width: 95%;
  height: 70rpx;
  border-radius: 40rpx;
  border: 1px solid #bbb;
}
.searchBar-box.tap{
  background: rgba(255,247,231,.5);
}
.search-plac{
  text-align: center;
  font-size: 11pt;
  margin-top: 4rpx;
}
.searchBar-box .search-icon{
  position: absolute;
  right: 10%;
  top: 35rpx;
  width: 44rpx;
  height: 44rpx;
}
.searchBar-box .search-input{
  height: 100%;
  padding-left: 20rpx
}